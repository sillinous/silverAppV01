# .env - Environment variables for Arbitrage OS

# Database Configuration
DATABASE_URL="postgresql://user:password@localhost/arbitrage_db"

# OpenAI API Key (for AI analysis and geocoding cleanup)
OPENAI_API_KEY="your_openai_api_key_here"

# Metals-API Key (for silver spot price)
METALS_API_KEY="your_metals_api_key_here"

# Route4Me API Key (for route optimization)
ROUTE4ME_API_KEY="your_route4me_api_key_here"

# Ximilar API Keys (for image analysis)
XIMILAR_API_TOKEN="your_ximilar_api_token_here"
XIMILAR_WORKSPACE_ID="your_ximilar_workspace_id_here"
XIMILAR_TASK_ID="your_ximilar_task_id_here"
